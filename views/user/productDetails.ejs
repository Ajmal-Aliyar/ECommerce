    <script>
        function handleCartButton(productId, userId) {
            const buttonText = document.getElementById(`button-text-${productId}`);
            if (buttonText.innerText === 'ADD TO CART') {
                addToCart(productId, userId);
                buttonText.innerText = 'GO TO CART';
            } else {
                goToCart();
            }
        }

        function addToCart(productId, userId) {
            fetch('/addToCart', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ productId, userId })
            })
                .then(response => response.json())
                .then(data => {
                    console.log(data);
                    if (data.status) {
                        const buttonText = document.getElementById(`button-text-${productId}`);
                        buttonText.innerText = 'GO TO CART';
                        console.log(`Item ${productId} added to cart`);

                    } else {
                        const buttonText = document.getElementById(`button-text-${productId}`);
                        buttonText.innerText = 'ADD TO CART';
                        Swal.fire({
                            title: "Currently no stock left !",
                            showClass: {
                                popup: `
                                animate__animated
                                animate__fadeInUp
                                animate__faster
                                `
                            },
                            hideClass: {
                                popup: `
                                animate__animated
                                animate__fadeOutDown
                                animate__faster
                                `
                            }
                        });
                    }
                })
                .catch(error => console.error('Error:', error));
        }

        function goToCart() {
            window.location.href = '/cart';
        }
    </script>

