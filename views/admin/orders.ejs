        <script>
            document.querySelectorAll('[id^="active-btn-"]').forEach(function (element) {
                element.addEventListener('click', function () {
                    const offerId = this.id.split('-')[2]; // Extract the offer ID from the element ID
                    const swalWithBootstrapButtons = Swal.mixin({
                        customClass: {
                            confirmButton: "btn btn-success",
                            cancelButton: "btn btn-danger"
                        },
                        buttonsStyling: false
                    });
                    swalWithBootstrapButtons.fire({
                        title: "Confirm the request ?",
                        text: "",
                        icon: "question",
                        confirmButtonText: "Yes",
                        cancelButtonText: "",
                        reverseButtons: true
                    }).then((result) => {
                        if (result.isConfirmed) {
                            fetch('/admin/returnOrder', {
                                method: 'post',
                                headers: {
                                    'Content-Type': 'application/json'
                                },
                                body: JSON.stringify({
                                    id: offerId
                                })
                            }).then(response => response.json())
                                .then(data => {
                                    console.log(data);
                                    if (data.status) {

                                        swalWithBootstrapButtons.fire({
                                            title: "Activated!",
                                            text: "The offer has been activated.",
                                            icon: "success"
                                        })
                                        const element = document.getElementById(`active-btn-${data.id}`);
                                            if (element) {
                                                element.innerHTML = "<div class='badge ' style='background-color:#9b9ca4;'>approved</div>";
                                            } else {
                                                console.error('Element not found:', `active-btn-${data.id}`);
                                            }
                                    }
                                }).catch(error => {
                                    swalWithBootstrapButtons.fire({
                                        title: "Error!",
                                        text: "There was a problem with the request.",
                                        icon: "error"
                                    });
                                });
                        } else if (result.dismiss === Swal.DismissReason.cancel) {
                            swalWithBootstrapButtons.fire({
                                title: "Cancelled",
                                text: "The offer activation was cancelled.",
                                icon: "error"
                            });
                        }
                    });
                });
            });
            function reload() {
                window.location.href = '/admin/orders';
            }
            function more(id) {
                window.location.href = `/admin/orderDetails?id=${id}`

            }
            function dashboard() {
                window.location.href = '/admin/dashboard'
            }

            document.addEventListener('DOMContentLoaded', function () {
                const dateElements = document.querySelectorAll('.price-col');

                dateElements.forEach(function (element) {
                    const rawDate = new Date(element.getAttribute('data-date'));
                    const options = {
                        weekday: 'short',
                        year: 'numeric',
                        month: 'short',
                        day: 'numeric',
                        hour: '2-digit',
                        minute: '2-digit',
                        second: '2-digit'
                    };
                    const formattedDate = rawDate.toLocaleString('en-US', options).replace(/, /g, ' ');
                    const formattedDateElement = element.querySelector('.formatted-date');

                    if (formattedDateElement) {
                        formattedDateElement.innerText = formattedDate;
                    } else {
                        console.warn('Missing .formatted-date element inside .price-col');
                    }
                });
            });

            function updateOrderStatus(orderId) {


                const selectElement = document.getElementById(`orderStatus-${orderId}`);
                const newStatus = selectElement.value;
                if (newStatus != 'order placed') {
                    fetch('/admin/updateOrderStatus', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                        body: JSON.stringify({ orderId, newStatus })
                    })
                        .then(response => response.json())
                        .then(data => {
                            if (data.success) {
                                console.log('Order status updated successfully');
                            } else {
                                console.error('Failed to update order status');
                            }
                        })
                        .catch(error => {
                            console.error('Error:', error);
                        });
                }

            }


        </script>
