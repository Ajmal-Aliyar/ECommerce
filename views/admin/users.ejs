    <script>
        function status(stat, col) {
            const row = document.getElementById(stat)
            const cells = row.getElementsByTagName('td')
            for (let i = 0; i < cells.length; i++) {
                if (cells[i].style.color == 'red') {
                    cells[i].style.color = 'black'
                    if (i === 0) {
                        fetch('/admin/unblock', {
                            method: 'PATCH',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify({
                                id: stat
                            })
                        }).then(data => {
                            console.log('PATCH request succeeded with JSON response:', data);
                        }).catch(error => {
                            console.error('error', error);
                        });
                    }

                } else {
                    cells[i].style.color = 'red'
                    if (i === 0) {
                        fetch('/admin/block', {
                            method: 'PATCH',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify({
                                id: stat
                            })
                        }).then(data => {
                            console.log('PATCH request succeeded with JSON response:', data);
                        }).catch(error => {
                            console.error('error', error);
                        });
                    }

                }

            }
            console.log(cells)
        }
        function dashboard() {
                window.location.href = '/admin/dashboard'
            }
    </script>
