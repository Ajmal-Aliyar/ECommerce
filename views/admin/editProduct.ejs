    <div class="row justify-content-center  pt-5">

        <main class="col-lg-6  ">
            <div class="summary">
                <h3 class="summary-title text-center">Edit product</h3><!-- End .summary-title -->

                <div style=" padding: 1em;">
                    <form action="/admin/editProduct" method="POST" enctype="multipart/form-data">
                        <input type="hidden" name="isBlocked" value="false">
                        <input type="hidden" name="id" value="<%=product._id%>">
                        <label for="register-email">Product name *</label>
