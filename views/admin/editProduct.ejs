                            class="form-control">

                        <label for="register-email">Category *</label>
                        <select name="productCategory" id="size" class="form-control" required>
                            <%data.forEach((category)=>{
                                if(category.isBlocked==false){%>
                                <%if(product.productCategory==category.categoryName){%>
                                    
                                    <option name="<%=category.categoryName%>" selected>
                                        <%=category.categoryName%>
                                    </option>
                                    <%}else{%>
                                        <option name="<%=category.categoryName%>">
                                            <%=category.categoryName%>
                                        </option>
                                <%}}})%>
                        </select>


                        <div class="card shadow p-3  bg-white rounded"><a onclick="addFileInput()"
                                class="btn-outline-primary-2"
                                style="width: 100%; text-align: center; font-weight: 700;">ADD IMAGE</a></div>



                        <%for(let i=0 ; i<product.productImage.length;i++){%>


                            <div id="imageContainer">

                                <div class="file-input-container">
                                    <div style="width: 21em;">Image already exists for <%=product.productName%>
                                    </div>
                                    <input type="hidden" value="<%=product.productImage[i]%>" name="productImage[]">
                                    <button onclick="removeFileInput(this)" class="dlButton">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                            fill="currentColor" class="bi bi-trash" viewBox="0 0 16 16">
                                            <path
                                                d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5m2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5m3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0z" />
                                            <path
                                                d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1zM4.118 4 4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4zM2.5 3h11V2h-11z" />
                                        </svg>
                                    </button>
                                    <img class="preview" src="<%=product.productImage[i]%>" alt="Image Preview">
                                </div>
                            </div>
                            <%}%>
                                <script>
                                    function addFileInput() {
                                        const imageContainer = document.getElementById('imageContainer');
                                        const fileInputContainer = document.createElement('div');
                                        fileInputContainer.classList.add('file-input-container');
                                        const newInput = document.createElement('input');
                                        newInput.type = 'file';
                                        newInput.name = 'productImage[]';
                                        newInput.accept = 'image/*';

                                        newInput.addEventListener('change', function () {
                                            previewImage(this);
                                        });
                                        const removeButton = document.createElement('button');
                                        removeButton.innerHTML = `
                                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-trash" viewBox="0 0 16 16">
                                        <path d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5m2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5m3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0z"/>
                                        <path d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1zM4.118 4 4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4zM2.5 3h11V2h-11z"/>
                                    </svg>
                                `;
                                        removeButton.classList.add('dlButton');
                                        removeButton.addEventListener('click', function () {
                                            imageContainer.removeChild(fileInputContainer);
                                        });

                                        const imgPreview = document.createElement('img');
                                        imgPreview.classList.add('preview');
                                        imgPreview.src = '#';
                                        imgPreview.alt = 'Image Preview';

                                        fileInputContainer.appendChild(newInput);
                                        fileInputContainer.appendChild(removeButton);
                                        fileInputContainer.appendChild(imgPreview);

                                        imageContainer.appendChild(fileInputContainer);
                                    }

                                    function removeFileInput(button) {
                                        const fileInputContainer = button.parentNode;
                                        const imageContainer = fileInputContainer.parentNode;
                                        imageContainer.removeChild(fileInputContainer);
                                    }

                                    function previewImage(input) {
                                        const file = input.files[0];
                                        const imgPreview = input.nextElementSibling.nextElementSibling;
                                        const reader = new FileReader();

                                        reader.onload = function (e) {
                                            imgPreview.src = e.target.result;
                                        };

                                        reader.readAsDataURL(file);
                                    }
                                </script>

                                <label for="register-email">Price before *</label>
                                <input type="number" name="priceBefore" value="<%=product.productPrices.priceBefore%>"
                                    required class="form-control col-5">

                                <label for="register-email">Price after*</label>
                                <input type="number" name="priceAfter" value="<%=product.productPrices.priceAfter%>"
                                    required class="form-control col-5">

                                <label for="register-email">Stock *</label>
                                <input type="number" name="productStock" value="<%=product.productStock%>" required
                                    class="form-control">

                                    <label for="register-email">Cost *</label>
                                <input type="number" name="productCost" value="<%=product.productCost%>" required
