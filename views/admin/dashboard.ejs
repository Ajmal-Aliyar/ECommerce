    }

    function topCategory() {
      fetch('/admin/topCategory', {
        method: 'post',
        headers: {
          'Content-Type': 'application/json'
        }
      }).then(response => response.json())
        .then(data => {
          let catname=[]
          let catvalue=[]
          data.topCategory.forEach(item=>{
            catname.push(item._id)
            catvalue.push(item.sales)
          })
          const ctx = document.getElementById('top-category');

          new Chart(ctx, {
            type: 'pie',
            data: {
              labels: catname,
              datasets: [{
                label: 'category',
                data: catvalue,
                borderWidth: 1
              }]
            },
            options: {
              scales: {
                y: {
                  beginAtZero: true
                }
              }
            }
          });
        })
    }
    function topProducts(){
      fetch('/admin/topProducts',{
        method: 'post',
        headers: {
          'Content-Type': 'application/json'
        }
      }).then(response => response.json())
        .then(data => {
          console.log(data);
          let catname=[]
          let catvalue=[]
          data.topProducts.forEach(item=>{
            catname.push(item._id.split('–')[0])
            catvalue.push(item.sales)
          })
          const ctx = document.getElementById('top-product');

          new Chart(ctx, {
            type: 'polarArea',
            data: {
              labels: catname,
              datasets: [{
                label: 'product',
                data: catvalue,
                borderWidth: 1
              }]
            },
            options: {
              scales: {
                y: {
                  beginAtZero: true
                }
              }
            }
          });
        })
    }
